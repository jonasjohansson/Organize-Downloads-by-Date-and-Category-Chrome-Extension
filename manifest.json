{
    "name": "Organize Downloads by Date and Category",
    "description": "Organizes your download folder by date and category. Example: YEAR/CATEGORY/MONTH/DAY/FILE",
    "version": "1.1",
    "permissions": [
        "downloads",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "manifest_version": 3
}
